<!DOCTYPE html>
<html lang="en">
<head>
    <% include elements/tag_head.ejs%>

</head>
<body>
<!--
	Upper Header Section
-->
<% include elements/upper_header.ejs%>
<!--
Lower Header Section
-->
<div class="container">
    <% include elements/lower_header.ejs%>

    <!--
    Navigation Bar Section
    -->
    <% include elements/navbar.ejs%>
    <!--
    Body Section
    -->
    <div class="row">
        <% include elements/sidebar.ejs%>
        <div class="span9">
            <ul class="breadcrumb">
                <li><a href="/">Trang chủ</a> <span class="divider">/</span></li>
                <li class="active">Tài khoản</li>
            </ul>
            <h3> Tài khoản</h3>
            <hr class="soft"/>


            <div class="well">
                <form class="form-horizontal" >
                    <h3>Thông tin tài khoản</h3>
                    <div class="control-group">
                        <label class="control-label">Tên<sup>*</sup></label>
                        <div class="controls">
                            <input type="text" placeholder="">
                        </div>
                    </div>
                    <div class="control-group">
                        <label class="control-label">Nghề nghiệp<sup>*</sup></label>
                        <div class="controls">
                            <input type="text" placeholder="">
                        </div>
                    </div>
                    <div class="control-group">
                        <label class="control-label">Quốc gia<sup>*</sup></label>
                        <div class="controls">
                            <input type="text" placeholder="">
                        </div>
                    </div>
                    <div class="control-group">
                        <label class="control-label">Email<sup>*</sup></label>
                        <div class="controls">
                            <input type="text" placeholder="">
                        </div>
                    </div>
                    <div class="control-group">
                        <label class="control-label">Địa chỉ<sup>*</sup></label>
                        <div class="controls">
                            <input type="text" placeholder="">
                        </div>
                    </div>
                    <div class="control-group">
                        <label class="control-label">Số điện thoại<sup>*</sup></label>
                        <div class="controls">
                            <input type="text" placeholder="">
                        </div>
                    </div>
                    <div class="control-group">
                        <div class="controls">
                            <input onclick="successUpdateAccount()" type="submit" name="submitAccount" value="Đống ý" class="exclusive shopBtn">
                        </div>
                    </div>
                </form>
            </div>


        </div>
        <!--
        Clients
        -->
        <% include elements/client.ejs%>
        <!--
        Footer
        -->
        <% include elements/footer.ejs%>

    </div><!-- /container -->

    <% include elements/copyright.ejs%>
    <a href="#" class="gotop"><i class="icon-double-angle-up"></i></a>
    <!-- Placed at the end of the document so the pages load faster -->
    <% include elements/script_end.ejs%>
</div>
</body>
</html>
